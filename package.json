{
  "name": "m3-mixins-for-less",
  "description": "Material Design 3 components styles for using with LESS",
  "version": "1.0.0",
  "main": "app.tsx",
  "keywords": [
    "material design 3",
    "react components"
  ],
  "author": "Rus Sharafiev",
  "license": "ISC",
  "scripts": {
    "dev": "concurrently --names 'esbuild,less' -c 'yellow,blue' \"esbuild app.tsx --outfile=build/index.js --bundle --watch --external:*.woff2 --external:*.woff \" \"less-watch-compiler styles build index.less\"",
    "build": "esbuild index=app.tsx build/index.css --outdir=build --allow-overwrite --bundle --minify --external:*.woff2 --external:*.woff"
  },
  "dependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.10",
    "less": "^4.1.3",
    "less-watch-compiler": "^1.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "typescript": "^4.9.4"
  }
}
